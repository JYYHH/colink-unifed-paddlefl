{
    "framework": "paddlefl",
    "dataset": "breast_horizontal",
    "algorithm": "fedavg",
    "model": "mlp_128",
    "bench_param": {
        "mode": "local",
        "device": "cpu"
    },
    "training": {
        "global_epochs": 3,
        "batch_size": 32,
        "learning_rate": 0.01,
        "local_epochs": 1,
        "optimizer": "sgd",
        "client_per_round": 2,
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    }
}